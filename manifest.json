{
    "name": "darkMe",
    "description": "a simple extension that detects Malacious files in website. ",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "iGoDark"

    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["action.css"]

    }]
}